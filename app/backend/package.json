{
  "name": "teste-cashforce-backend",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rescue": "^1.2.0",
    "mysql2": "2.1.0",
    "sequelize": "6.3.4"
  },
  "main": "src/server.js",
  "scripts": {
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "start": "node .",
    "drop": "npx sequelize-cli db:drop",
    "test": "nyc --all --include models --include services --include controllers mocha ./src/tests/**/*.js --exit",
    "debug": "nodemon .",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-trybe-backend": "1.0.3",
    "eslint-plugin-import": "2.22.1",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "6.2.0",
    "sequelize-jest-helpers": "^1.0.2",
    "sinon": "^11.1.1",
    "supertest": "4.0.2",
    "wait-on": "^6.0.1"
  }
}
